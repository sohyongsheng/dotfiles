[core]
    editor = vim

[alias]
    cho = checkout
    sw = switch
    br = branch
    ci = commit
    st = status
    re = remote
    unstage = reset HEAD --
    last = log -1
    lg = log --oneline 
    ls = log --oneline --graph --all
    li = log --oneline --graph --all --simplify-by-decoration
    lo = log --oneline --graph --simplify-by-decoration
    top = rev-parse --show-toplevel

